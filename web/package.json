{
  "name": "rapid-photo-web",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "amplify:sandbox": "npx ampx sandbox",
    "amplify:deploy": "npx ampx sandbox --outputs-out-dir src",
    "adminer:start": "docker start rapidphoto-adminer 2>/dev/null || docker run -d --name rapidphoto-adminer --link rapidphoto-postgres:db -p 8081:8080 adminer",
    "adminer:stop": "docker stop rapidphoto-adminer"
  },
  "dependencies": {
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-toast": "^1.2.15",
    "@tanstack/react-query": "^5.66.1",
    "@tanstack/react-virtual": "^3.11.1",
    "aws-amplify": "^6.13.2",
    "axios": "^1.7.9",
    "clsx": "^2.1.1",
    "fflate": "^0.8.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.5",
    "react-router-dom": "^7.1.0",
    "tailwind-merge": "^2.6.0",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.17.0",
    "@aws-amplify/backend-cli": "^1.8.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "happy-dom": "^20.0.10",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.3",
    "vite": "^6.0.7",
    "vitest": "^4.0.8"
  }
}
